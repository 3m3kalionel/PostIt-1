{
  "name": "post-it",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for notifications",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "start:dev": "babel-watch server/index.js",
    "migrations": "sequelize db:migrate",
    "transpile": "babel **/*.js --ignore node_modules --out-dir dist --copy-files",
    "test": "nyc mocha server/tests/**/*.js --timeout 10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomipaul/PostIt.git"
  },
  "keywords": [
    "PostIt",
    "notifications",
    "groups",
    "messages",
    "nodejs",
    "reactjs",
    "redux"
  ],
  "author": "Oluwatomi Akande <tomipaul95@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomipaul/PostIt/issues"
  },
  "homepage": "https://github.com/tomipaul/PostIt#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-watch": "^2.0.6",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "chai-uuid": "^1.0.6",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "keypair": "^1.0.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "node-env-file": "^0.1.8",
    "node-rsa": "^0.4.2",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.1.0",
    "sequelize-cli": "^2.7.0"
